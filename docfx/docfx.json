{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "FastBertTokenizer/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      },
      {
        "files": ["logo.svg", "logo-darkmode.svg"],
        "src": "../"
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appTitle": "FastBertTokenizer",
      "_appFooter": "Â© Copyright Georg Jung",
      "_enableSearch": true,
      "pdf": false
    }
  }
}
